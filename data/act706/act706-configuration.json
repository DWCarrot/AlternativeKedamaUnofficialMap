{
  "world": "act706",
  "default": true,
  "crs": {
    "class": "L.AKM.CRS",
    "args": [
      {
        "scale": 0.5,
        "offset": [0, 0],
        "maxZoom": 5
      }
    ]
  },
  "baselayer": {
    "class": "L.TileLayer",
    "args": [
      "https://kedama-map-tiles.jsw3286.eu.org/act706/z{z}/voxelmap/{x},{y}.png",
      {
        "tileSize": 512,
        "minZoom": 1,
        "maxZoom": 5,
        "attribution": "&copy; 2018-2019 <a href=\"https://www.craft.moe\">Kedama-Koiru Monogatari</a>"
      }
    ]
  },
  "overlayers": {
    "map-marker": {
      "class": "L.AKM.MarkerControlLayer",
      "autoshow": true,
      "args": [
        "https://kedama-map-markers.jsw3286.eu.org/getMarkers?map=act706",
        {
          "akm-id": "marker_control_act706_001",
          "markerFactory": "$MARKER_YAKM",
          "searchUI": "$SEARCH_UI_YAKM",
          "editUI": "$EDIT_UI_YAKM",
          "pushOperation": "$PUSH_OP_YAKM",
          "loadOperation": "$LOAD_OP_YAKM",
          "dataPro1": "$DATA_PRO_1_YAKM",
          "dataPro2": "$DATA_PRO_2_YAKM"
        }
      ]
    }
  }
}

