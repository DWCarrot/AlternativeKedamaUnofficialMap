{
  "world": "v3(vanilla)",
  "default": false,
  "crs": {
    "class": "L.AKM.CRS",
    "args": [
      {
        "scale": 1,
        "offset": [-64, -64],
        "maxZoom": 3
      }
    ]
  },
  "baselayer": {
    "class": "L.TileLayer",
    "args": [
      "../data/v3-vanilla/{z}/{x},{y}.png",
      {
        "tileSize": 512,
        "maxZoom": 3,
        "attribution": "&copy; 2018 <a href=\"https://www.craft.moe\">Kedama-Koiru Monogatari</a>"
      }
    ]
  },
  "overlayers": {
    "map-marker": {
      "class": "L.AKM.MarkerControlLayer",
      "autoshow": true,
      "args": [
        "../data/v3/v3-markers.json",
        {
          "searchUI": "$SEARCH_UI",
          "editUI": "$EDIT_UI",
          "pushOperation": "$PUSH_OP"
        }
      ]
    }
  }
}

